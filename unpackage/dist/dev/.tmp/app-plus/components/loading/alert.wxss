
@font-face {font-family: "simple-icon";
  src:url('data:application/x-font-woff;charset=utf-8;base64,');
}
.simple-icon {
  font-family:"simple-icon" !important;
  font-size:36rpx;
  font-style:normal;
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
  width: 30rpx;
  height: 30rpx;
}
@-webkit-keyframes simple-show{from {top:-60px;}
to {top:0;}}
@keyframes simple-show{from {top:-60px;}
to {top:0;}}
.animate-show{-webkit-animation: simple-show 300ms linear forwards;animation: simple-show 300ms linear forwards;}
@-webkit-keyframes simple-hide{from {top:0;}
to {top:-120px;}}
@keyframes simple-hide{from {top:0;}
to {top:-120px;}}
.animate-hide{-webkit-animation: simple-hide 300ms linear forwards;animation: simple-hide 300ms linear forwards;}
.icon-error:before { content: "\E604";}
.icon-right:before { content: "\E60F";}
.icon-msg:before { content: "\E623";}
.simple-alert{display:-webkit-box;display:-webkit-flex;display:-ms-flexbox;display:flex; width:100%; padding:20rpx 0%; -webkit-flex-wrap:nowrap; -ms-flex-wrap:nowrap; flex-wrap:nowrap; background:#FFF; position:fixed; z-index:99999; left:0; top:-60px;}
.simple-alert .right{color:#00B26A !important;}
.simple-alert .error{color:#FF0000 !important;}
.simple-alert .msg{color:#00B26A !important;}
.simple-alert .icon{width:35rpx; line-height:35rpx; -webkit-box-pack:center; -webkit-justify-content:center; -ms-flex-pack:center; justify-content:center; -webkit-flex-shrink:0; -ms-flex-negative:0; flex-shrink:0; margin:0 12px;}
.simple-alert-msg{line-height:35rpx; font-size:35rpx; overflow:hidden; -webkit-box-pack:start; -webkit-justify-content:flex-start; -ms-flex-pack:start; justify-content:flex-start; width:100%; white-space:nowrap; -o-text-overflow:ellipsis; text-overflow:ellipsis;color: #303133}

