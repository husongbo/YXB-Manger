/* ball-scale */
.mht-loader-ball-scale {
	width: 50px;
	height: 50px;
	-webkit-animation: ball-scale infinite linear .75s;
	animation: ball-scale infinite linear .75s;
	border-radius: 100%;
	background-color: #007aff
}
@-webkit-keyframes ball-scale {
0% {
		-webkit-transform: scale(.1);
		-ms-transform: scale(.1);
		transform: scale(.1);
		opacity: 1
}
100% {
		-webkit-transform: scale(1);
		-ms-transform: scale(1);
		transform: scale(1);
		opacity: 0
}
}
@keyframes ball-scale {
0% {
		-webkit-transform: scale(.1);
		-ms-transform: scale(.1);
		transform: scale(.1);
		opacity: 1
}
100% {
		-webkit-transform: scale(1);
		-ms-transform: scale(1);
		transform: scale(1);
		opacity: 0
}
}

/* ball-circle */
.mht-loader-ball-circle {
	margin-top: 5px;
	position: relative;
	width: 40px;
	height: 40px
}
.mht-loader-ball-circle:after,
.mht-loader-ball-circle:before {
	position: absolute;
	width: 10px;
	height: 10px;
	content: '';
	border-radius: 100%;
	background-color: #007aff
}
.mht-loader-ball-circle:before {
	-webkit-transform: translate(0, 0);
	    -ms-transform: translate(0, 0);
	        transform: translate(0, 0);
	-webkit-animation: ball-circle-before infinite 1.5s linear;
	animation: ball-circle-before infinite 1.5s linear
}
.mht-loader-ball-circle:after {
	-webkit-transform: translate(30px, 30px);
	    -ms-transform: translate(30px, 30px);
	        transform: translate(30px, 30px);
