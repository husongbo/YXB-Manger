.content {
  height: 100%;
  background: white;
}
.content .uni-list-cell {
    background: white;
    border-bottom: 1px solid #e1e1e1;
}
.content .uni-list-cell::after {
    height: 0;
}
.content .cell-btn {
    padding: 11px 15px;
    border: 0;
}
.content .zd-query {
    padding: 0.6rem 0.4rem 0.4rem 0.4rem;
}
.content .zd-query .list-block {
      margin: 0;
      font-size: 0.85rem;
}
.content .zd-query .list-block ._ul {
        background: #fff;
        list-style: none;
        padding: 0;
        margin: 0;
        position: relative;
        overflow: hidden;
        border-radius: 0.3rem;
        -webkit-box-shadow: 0 0 0.6rem rgba(136, 130, 129, 0.3);
                box-shadow: 0 0 0.6rem rgba(136, 130, 129, 0.3);
}
.content .zd-query .list-block ._ul .item-content {
          border-bottom: 1px solid #eee;
          -webkit-box-sizing: border-box;
                  box-sizing: border-box;
}
.content .zd-query .list-block ._ul .item-left {
          float: left;
          height: 50px;
          line-height: 50px;
          padding: 0 30rpx;
          font-size: 0.8rem;
}
.content .zd-query .list-block ._ul .item-right {
          float: right;
          height: 50px;
          line-height: 50px;
          padding: 0 30rpx;
          color: #ff8200;
          font-size: 0.8rem;
}
.content .zd-query .list-block ._ul .item-next {
          width: 26px;
          height: 26px;
          margin-top: 12px;
}
.content .zd-query .list-block ._ul:active {
        background: #fafafa;
}

